{
  "name": "i18next-compose",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/arthur-plazanet/i18next-compose.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "module": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ."
  },
  "devDependencies": {
    "typescript": "~5.9.0",
    "vite": "7.1.3",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.0.7"
  },
  "dependencies": {
    "i18next": "25.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
